const Vector = fn (dimensions: number) any {
	var i = 0;
	const ret: any = [];
	while (i < dimensions) {
		ret[i] = number;
		i += 1;
	}
	return ret;
};

const u: Vector(2) = [1, 2];
println("         u = " + to_string(u));
println("decltype u = " + to_string(decltype u));

println("");

